        uniswapV2Pair_USDC = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), USDC);
        uniswapV2Pair_BUSD = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), BUSD);
        uniswapV2Pair_WBNB = IUniswapV2Factory(_uniswapV2Router.factory()).createPair(address(this), WBNB);

        _uniswapV2PairList[uniswapV2Pair_USDT]=true;
        _uniswapV2PairList[uniswapV2Pair_USDC]=true;
        _uniswapV2PairList[uniswapV2Pair_BUSD]=true;
        _uniswapV2PairList[uniswapV2Pair_WBNB]=true;

        //exclude owner and this contract from fee
        _isExcludedFromFees[msg.sender] = true;
        _isExcludedFromFees[deadAddr] = true;
        _isExcludedFromFees[nodeAddr] = true;
        _isExcludedFromFees[marketAddr] = true;
        _isExcludedFromFees[destroyAddr] = true;
        _isExcludedFromFees[bronzeAddr] = true;
        _isExcludedFromFees[goldAddr] = true;
        _isExcludedFromFees[kingAddr] = true;
        _isExcludedFromFees[address(this)] = true;
        
        takeFee=false;

        _mint(msg.sender, 10000000*1e18);
        
    }

    function excludeFromFees(address account, bool excluded) public onlyOwner {
        require(
            _isExcludedFromFees[account] != excluded,
            "CSDAOToken:account is already the value of 'excluded'"
        );
        _isExcludedFromFees[account] = excluded;
    }
    
    function setblackList(address account, bool flag) public onlyOwner {
        require(
            _blackList[account] != flag,
            "CSDAOToken:account is already the value of 'flag'"
        );
        _blackList[account] = flag;
    }

    function setUniswapV2PairList(address routerAddr,bool flag) public onlyOwner {
        require(
            _uniswapV2PairList[routerAddr] != flag,
            "CSDAOToken:routerAddr is already the value of 'flag'"
        );
        _uniswapV2PairList[routerAddr] = flag;
    }

    function setTakeFee(bool _takeFee) public onlyOwner {
        takeFee=_takeFee;
    }

    function setFeeAddr(uint8 index ,address _newAddr) public onlyOwner {
        
        if(index==1){
            nodeAddr     =_newAddr;
        }else if(index==2){
            marketAddr   =_newAddr;
        }else if(index==3){
            destroyAddr  =_newAddr;
        }else if(index==4){
            bronzeAddr   =_newAddr;
        }else if(index==5){
            goldAddr     =_newAddr;
        }else if(index==6){
            kingAddr     =_newAddr;
        }               
    }

    function _transfer(address from,address to,uint256 amount) internal override {
    
        require(!_blackList[from] && !_blackList[to],"CSDAOToken:blacklist address");

        if (amount == 0) {
            super._transfer(from, to, 0);
            return;
        }

        bool takeFeeflag=takeFee;

        if (_isExcludedFromFees[from] || _isExcludedFromFees[to]) {
            takeFeeflag = false;
        }
        
        if (_uniswapV2PairList[from]) { 
            //buy
            if (takeFeeflag){
                //sub amount for nodeFee marketFee destroyFee
                uint256 nodeFeeNum=amount*nodeFee/100;
                uint256 marketFeeNum=amount*marketFee/100;
                uint256 destroyFeeNum=amount*destroyFee/100;

                super._transfer(from, nodeAddr, nodeFeeNum);
                super._transfer(from, marketAddr, marketFeeNum);
                super._transfer(from, destroyAddr, destroyFeeNum);
                super._transfer(from, to, amount-nodeFeeNum-marketFeeNum-destroyFeeNum);

            }else{
                super._transfer(from, to, amount);
            }

        }else if (_uniswapV2PairList[to]) { 
            //sell
            if (takeFeeflag){
                //sub amount for bronzeFee goldFee kingFee
                uint256 bronzeFeeNum=amount*bronzeFee/100;
                uint256 goldFeeNum=amount*goldFee/100;
                uint256 kingFeeNum=amount*kingFee/100;

                super._transfer(from, bronzeAddr, bronzeFeeNum);
                super._transfer(from, goldAddr, goldFeeNum);
                super._transfer(from, kingAddr, kingFeeNum);
                super._transfer(from, to, amount-bronzeFeeNum-goldFeeNum-kingFeeNum);

            }else{
                super._transfer(from, to, amount);
            }

        }else{
            //other
            super._transfer(from, to, amount);
        }
    
    }
    
    receive() external payable {}

    function takeoutETH() public onlyOwner {
        require(address(this).balance>0,'CSDAOToken:balance is zero');
        payable(msg.sender).transfer(address(this).balance);
    }

    function takeoutToken(address token,address to) public onlyOwner {
        require(to !=address(0), 'CSDAOToken:to address is zero');

        uint256 balance=IERC20(token).balanceOf(address(this));
        require(balance>0,'CSDAOToken:balanceOf is zero');

        if(balance > 0){
            IERC20(token).transfer(to, balance);
        }
    }

}